<?xml version="1.0" encoding="utf-8"?>
<ApplicationManifest ApplicationTypeName="dreamhomesf" ApplicationTypeVersion="1.0.0"
    xmlns="http://schemas.microsoft.com/2011/01/fabric"
    xmlns:xsd="http://www.w3.org/2001/XMLSchema"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
    <Parameters>
      <Parameter Name="dhclient_InstanceCount" DefaultValue="1" />
      <Parameter Name="dhnotification_InstanceCount" DefaultValue="1" />
    </Parameters>
    <ServiceManifestImport>
        <ServiceManifestRef ServiceManifestName="dhNotification" ServiceManifestVersion="1.0.0" />
    </ServiceManifestImport>
    <ServiceManifestImport>
        <ServiceManifestRef ServiceManifestName="dhClient" ServiceManifestVersion="1.0.0" />
    </ServiceManifestImport>
    <DefaultServices>
      <Service Name="dhNotification">
          <StatelessService ServiceTypeName="dhnotificationType" InstanceCount="[dhnotification_InstanceCount]">
              <SingletonPartition />
          </StatelessService>
      </Service>
      <Service Name="dhClient">
          <StatelessService ServiceTypeName="dhclientType" InstanceCount="[dhclient_InstanceCount]">
              <SingletonPartition />
          </StatelessService>
      </Service>
    </DefaultServices>
</ApplicationManifest>
